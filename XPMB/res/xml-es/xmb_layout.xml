            id="NeoGeo Arcade"
            icon="ui_xmb_game_folder" />
    </item>
    <item
        id="Web"
        icon="ui_xmb_web">
        <subitem
            id="Navegador web"
            exec="com.android.browser/.BrowserActivity"
            icon="ui_xmb_web_browser"
            type="exec" />
        <subitem
            id="Búsqueda por internet"
            exec="com.google.android.googlequicksearchbox/.SearchActivity"
            icon="ui_xmb_web_search"
            type="exec" />
    </item>
    <item
        id="Aplicaciones"
        icon="ui_xmb_apps">
        <subitem
            id="Aplicaciones instaladas"
            icon="ui_xmb_apps_installed"
            submenu="XPMB_Submenu_APP"
            type="submenu.half" >
        </subitem>
