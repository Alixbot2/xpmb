<?xml version="1.0" encoding="utf-8"?>
<xpmbroot>

    <item
        id="Ajustes"
        icon="ui_xmb_settings">
        <subitem
            id="Ajustes del sistema"
            exec="com.android.settings/.Settings"
            icon="ui_xmb_settings_system"
            type="exec" />
    </item>
    <item
        id="Fotos"
        icon="ui_xmb_photo">
        <subitem
            id="Cámara"
            exec="com.android.gallery3d/com.android.camera.CameraLauncher"
            icon="ui_xmb_photo_camera"
            type="exec" />
        <subitem
            id="Memoria externa"
            icon="ui_xmb_external_memory" />
    </item>
    <item
        id="Música"
        icon="ui_xmb_music">
        <subitem
            id="Memoria externa"
            icon="ui_xmb_external_memory"
            submenu="XPMB_Submenu_MUSIC"
            type="submenu.highlight" />
    </item>
    <item
        id="Video"
        icon="ui_xmb_video">
        <subitem
            id="Memoria externa"
            icon="ui_xmb_external_memory" />
    </item>
    <item
        id="Juegos"
        icon="ui_xmb_games">
        <subitem
            id="Nintendo Entertainment System"
            icon="ui_xmb_game_folder"
            submenu="XPMB_Submenu_NES"
            type="submenu.half" />
        <subitem
            id="Super Nintendo"
            icon="ui_xmb_game_folder" />
        <subitem
            id="Gameboy Advance"
            icon="ui_xmb_game_folder"
            submenu="XPMB_Submenu_GBA"
            type="submenu.half" />
