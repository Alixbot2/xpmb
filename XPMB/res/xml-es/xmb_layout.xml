<?xml version="1.0" encoding="utf-8"?>
<xpmbroot>

    <category
        icon="theme.icon|icon_setting"
        label="@strCategorySettings" >
        <subitem
            exec="com.android.settings/.Settings"
            icon="theme.icon|icon_system_setting"
            label="Ajustes del sistema" />
    </category>
    <category
        icon="theme.icon|icon_photo"
        label="@strCategoryPhoto" >
        <subitem
            action="android.media.action.IMAGE_CAPTURE"
            icon="theme.icon|icon_usbcamera"
            label="Cámara"
            type="exec" />
    </category>
    <category
        icon="theme.icon|icon_music"
        label="@strCategryMusic" >
        <subcategory
            anim="highlight"
            module="com.xpmb.media.music"
            icon="theme.icon|icon_sd"
            label="Memoria externa" />
    </category>
    <category
        icon="theme.icon|icon_video"
        label="@strCategoryVideo" >
        <subcategory
            icon="theme.icon|icon_sd"
            label="Memoria externa" />
    </category>
    <category
        icon="theme.icon|icon_game"
        label="@strCategoryGame" >
        <subcategory
            anim="half"
            module="com.xpmb.emu.nes"
            icon="theme.icon|icon_savedata"
            label="@strCategoryNES" />
        <subcategory
            anim="half"
            module="com.xpmb.emu.gba"
            icon="theme.icon|icon_savedata"
            label="@strCategoryGBA" />
    </category>
    <category
        icon="theme.icon|icon_network"
        label="@strCategoryWWW" >
        <subitem
            exec="com.android.browser/.BrowserActivity"
            icon="theme.icon|icon_browser"
            label="Navegador web" />
        <subitem
            exec="com.google.android.googlequicksearchbox/.SearchActivity"
            icon="theme.icon|icon_inet_search"
            label="Búsqueda por internet" />
    </category>
    <category
        icon="theme.icon|icon_user"
        label="@strCategoryApps" >
        <subcategory
            anim="half"
            icon="theme.icon|icon_download"
            label="Aplicaciones instaladas"
            module="com.xpmb.system.apps" />

    </category>

</xpmbroot>